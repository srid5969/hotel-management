{
	"name": "furation",
	"version": "1.0.0",
	"description": "First edit `.env` file",
	"main": "index.js",
	"scripts": {
		"start": "nodemon build/server.js",
		"build": "npm run clean && tsc",
		"clean": "shx rm -rf build",
		"dev": "ts-node-dev ./src/server.ts",
		"start:dev": " ts-node-dev -r tsconfig-paths/register src/server.ts ",
		"start:prod": "pm2  start ./build/server.js",
		"start:test": "nodemon  sh run.sh",
		"stop:prod": "pm2  stop ./build/server.js",
		"restart:prod": "pm2  restart ./build/server.js --watch",
		"monit:prod": "pm2  monit ./build/server.js --watch",
		"test": "jest",
		"restart": "npm run build && npm run restart:prod"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@leapjs/common": "^0.5.38",
		"@leapjs/core": "^0.5.38",
		"@leapjs/router": "^0.5.38",
		"@typegoose/typegoose": "^11.2.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"helmet": "^7.0.0",
		"inversify": "^6.0.1",
		"inversify-express-utils": "^6.4.3",
		"jsonwebtoken": "^9.0.0",
		"module-alias": "^2.2.2",
		"mongodb": "^5.5.0",
		"mongoose": "^7.2.1",
		"morgan": "^1.10.0",
		"reflect-metadata": "^0.1.13",
		"uuid": "^9.0.0",
		"winston": "^3.9.0"
	},
	"devDependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/morgan": "^1.9.4",
		"@types/node": "^20.2.5",
		"@types/uuid": "^9.0.1",
		"nodemon": "^2.0.22",
		"shx": "^0.3.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"_moduleAliases": {
		"app": "build/app",
		"configuration": "build/configuration",
		"common": "build/common",
		"resources": "build/resources",
		"build": "build"
	}
}
